public class MaxHeap<T extends Comparable<? super T>>
    implements HeapInterface<T> {

    private T[] arr;
    private int size;
    // Do not add any more instance variables

    /**
     * Creates a MaxHeap.
     */
    public MaxHeap() {

    }

    @Override
    public void add(T item) {

    }

    @Override
    public T remove() {

    }

    @Override
    public boolean isEmpty() {

    }

    @Override
    public int size() {

    }

    @Override
    public void clear() {

    }

    @Override
    public Comparable[] getBackingArray() {
        return arr;
    }
}
